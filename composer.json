{
	"name": "Busybee",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Rayner",
            "email": "hillrange@craigrayner.com"
        }
    ],
    "require": {
		"php": "^7.1|^7.2",
		"hillrange/ckeditor": "dev-master",
		"hillrange/gibbon": "dev-master",
		"hillrange/security": "dev-master",
		"hillrange/symfony-form": "dev-master",
		"knpuniversity/oauth2-client-bundle": "^1.16",
		"league/oauth2-google": "^2.0",
		"sensio/framework-extra-bundle": "^5.1",
		"sensiolabs/security-checker": "^4.1",
		"spipu/html2pdf": "^5.0",
		"symfony/asset": "^4.0",
		"symfony/config": "^4.0",
		"symfony/console": "^4.0",
		"symfony/debug-bundle": "^4.0",
		"symfony/expression-language": "^4.0",
		"symfony/flex": "^1.0",
		"symfony/form": "^4.0",
		"symfony/framework-bundle": "^4.0",
		"symfony/lts": "^4.0",
		"symfony/maker-bundle": "^1.0",
		"symfony/monolog-bundle": "^3.1",
		"symfony/orm-pack": "^1.0",
		"symfony/profiler-pack": "^1.0",
		"symfony/property-access": "^4.0",
		"symfony/requirements-checker": "^1.0",
		"symfony/security-bundle": "^4.0",
		"symfony/security-csrf": "^4.0",
		"symfony/swiftmailer-bundle": "^3.1",
		"symfony/templating": "^4.0",
		"symfony/translation": "^4.0",
		"symfony/twig-bridge": "^4.0",
		"symfony/twig-bundle": "^4.0",
		"symfony/validator": "^4.0",
		"symfony/var-dumper": "^4.0",
		"symfony/yaml": "^4.0",
		"twig/extensions": "^1.5",
		"twig/twig": "^2.4"
	},
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
			"requirements-checker": "script"
		},
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0J6PERM3GSPZ6J2SW55YM9W",
            "allow-contrib": false,
            "dev-master": "0.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
