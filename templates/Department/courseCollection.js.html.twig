{% trans_default_domain 'School' %}

<script type="text/javascript">
    $(".courseCollection").collection({
        name_prefix: '{{ form.courses.vars.id }}',
        allow_up: false,
        allow_down: false,
        add: '{{ addButton({transDomain: 'School', title: 'department.course.add'})|raw }}',
        remove: '{{ deleteButton({transDomain: 'School', title: 'department.course.remove', colour: 'warning'})|raw }}',
        add_at_the_end: true
    });
</script>