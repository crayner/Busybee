{% trans_default_domain "Calendar" %}

<script type="text/javascript" language="JavaScript">
    $(".calendarGradeList").collection({
        name_prefix: '{{ form.calendarGrades.vars.full_name }}',
        allow_up: true,
        allow_down: true,
        add: '{{ addButton({transDomain: 'Calendar', title: 'calendar_grade.add.title'})|raw }}',
        remove: '{{ deleteButton({transDomain: 'Calendar', title: 'calendar_grade.remove.title'})|raw }}',
        add_at_the_end: true
    });
</script>
