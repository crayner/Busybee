{% trans_default_domain "School" %}

<script type="text/javascript" language="JavaScript">
    $(".studentCollection").collection({
        name_prefix: '{{ form.students.vars.full_name }}',
        allow_up: false,
        allow_down: false,
        add: '{{ addButton({transDomain: 'School', title: 'activity.activity_student.add.title'})|raw }}',
        remove: '{{ deleteButton({transDomain: 'School', title: 'activity.activity_student.remove.title', colour: 'warning'})|raw }}',
        add_at_the_end: true
    });
</script>
